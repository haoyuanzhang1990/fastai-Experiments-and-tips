    "def train_spp(models:list):\n",
    "    names = ['fwd', 'bwd']\n",
    "    x = 0\n",
    "    for model in models:\n",
    "        lr = 1e-2\n",
    "        lr *= 64/48\n",
    "        \n",
    "        model.fit_one_cycle(1, lr, moms=(0.8,0.7))\n",
    "        model.unfreeze()\n",
    "        model.fit_one_cycle(10, lr/10, moms=(0.8,0.7))\n",
    "        \n",
    "        model.save(f'spp_{names[x]}_fine_tuned_10')\n",
    "        model.save_encoder(f'spp_{names[x]}_fine_tuned_enc_10')\n",
    "    return models"
   ]
